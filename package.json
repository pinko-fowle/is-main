{
  "name": "is-main",
  "version": "0.2.0",
  "description": "Check if current module is main module.",
  "scripts": {
    "lint": "eslint --ext js,md .",
    "test": "ava"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.4",
    "eslint": "^4.19.1",
    "eslint-config-concise": "^0.24.0",
    "eslint-config-concise-ava": "^0.23.0",
    "eslint-config-concise-esnext": "^0.24.1",
    "esm": "^3.0.28",
    "execa": "^0.10.0",
    "lint-staged": "^7.0.5",
    "preslint": "^0.23.1",
    "simple-git-hooks": "^2.7.0"
  },
  "repository": "ratson/is-main",
  "license": "MIT",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "*.md",
    "index.js"
  ],
  "keywords": [
    "main-module",
    "import",
    "require",
    "process",
    "main",
    "esm"
  ],
  "ava": {
    "babel": false,
    "require": [
      "esm"
    ]
  },
  "eslintConfig": {
    "extends": [
      "concise",
      "concise-esnext",
      "concise-ava"
    ]
  },
  "lint-staged": {
    "*.js": [
      "preslint --fix --git"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "npx ava"
  }
}
