{
  "name": "is-main",
  "version": "0.0.1",
  "description": "Check if current module is main module.",
  "scripts": {
    "lint": "eslint --ext js,md .",
    "test": "ava",
    "precommit": "lint-staged",
    "prepush": "ava"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^1.0.0-beta.4",
    "eslint": "^4.19.1",
    "eslint-config-concise": "^0.23.0",
    "eslint-config-concise-ava": "^0.23.0",
    "eslint-config-concise-esnext": "^0.22.0",
    "eslint-config-concise-jest": "^0.23.0",
    "esm": "^3.0.27",
    "execa": "^0.10.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.5",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.7.1"
  },
  "repository": "ratson/is-main",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "*.md",
    "index.js"
  ],
  "keywords": [
    "main-module",
    "import",
    "require",
    "process",
    "main",
    "esm"
  ],
  "ava": {
    "babel": false,
    "require": [
      "esm"
    ]
  },
  "eslintConfig": {
    "extends": [
      "concise",
      "concise-esnext",
      "concise-ava"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ]
  }
}
